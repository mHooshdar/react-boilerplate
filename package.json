    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint '**/*.scss'",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,scss,css}\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
